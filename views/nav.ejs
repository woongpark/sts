  <div class="panel-body">
    <button id="sim-run" type="button" class="btn btn-primary btn-lg btn-block" style="font-size:50%">Simulation Run</button>
    <button id="con-run" type="button" class="btn btn-primary btn-lg btn-block" style="font-size:50%">Control Run</button>
    <button type="button" class="btn btn-primary btn-lg btn-block" style="font-size:50%">Clear All</button>
